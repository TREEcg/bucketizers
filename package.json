{
  "name": "ldes-bucketizers",
  "private": true,
  "repository": "https://github.com/TREEcg/bucketizers",
  "scripts": {
    "build-changed": "lerna run build --since HEAD",
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "eslint": "eslint . --ext ts",
    "test": "npm run build && mocha -r ts-node/register 'packages/**/__tests__/**/*.ts'",
    "test:coverage": "npm run build && nyc mocha -r ts-node/register 'packages/**/__tests__/**/*.ts'",
    "eslint:cache": "npm run eslint -- --cache",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint -- --fix",
    "clean": "rm -rf ./node_modules && rm -rf ./packages/*/node_modules",
    "prepare": "husky install",
    "postinstall": "npm run bootstrap",
    "bootstrap": "lerna bootstrap",
    "lerna": "lerna",
    "watch": "tsc && tsc -w"
  },
  "devDependencies": {
    "@rdfjs/types": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-es": "^3.30.15",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unused-imports": "^1.1.5",
    "lerna": "^4.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "rdf-data-factory": "^1.1.0",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@treecg/types": "0.0.20",
    "husky": "^7.0.4",
    "winston": "^3.3.3"
  }
}
